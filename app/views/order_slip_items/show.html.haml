.row
  .col-md-12
    .page-header
      %h1
        Order Slip Item Detail
        = link_to edit_order_slip_item_path(@order_slip_item), class: "btn btn-primary btn-xs", "data-toggle" => "tooltip", "data-placement" => "top", title: "Edit order_slip_item" do
          %span.glyphicon.glyphicon-edit
        = link_to order_slip_items_path, class: "btn btn-default btn-xs", "data-toggle" => "tooltip", "data-placement" => "top", title: "Order Slip Items index" do
          %span.glyphicon.glyphicon-list
.row
  .col-md-2
    <strong>Quantity:</strong>
  .col-md-10
    = @order_slip_item.quantity
